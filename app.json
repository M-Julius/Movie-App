{
  "name": "MovieApp",
  "displayName": "MovieApp",
  "expo": {
    "name": "MovieApp",
    "slug": "MovieApp",
    "scheme": "movieapp",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-1x1.png",
    "splash": {
      "image": "./assets/images/splash-logo-app.png",
      "resizeMode": "contain",
      "backgroundColor": "#242424"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "eas": {
        "projectId": "3c4d5459-5ff9-43f9-8899-e44834a208aa"
      }
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "icon": "./assets/images/logo-1x1.png",
      "package": "com.movieapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-1x1.png",
        "backgroundImage": "./assets/images/app-icon-android-adaptive-background.png"
      },
      "splash": {
        "image": "./assets/images/splash-logo-app.png",
        "resizeMode": "contain",
        "backgroundColor": "#242424"
      }
    },
    "ios": {
      "icon": "./assets/images/logo-1x1.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.movieapp",
      "splash": {
        "image": "./assets/images/splash-logo-app.png",
        "tabletImage": "./assets/images/logo-1x1.png",
        "resizeMode": "contain",
        "backgroundColor": "#242424"
      }
    },
    "web": {
      "favicon": "./assets/images/logo-1x1.png",
      "splash": {
        "image": "./assets/images/logo-1x1.png",
        "resizeMode": "contain",
        "backgroundColor": "#242424"
      },
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  },
  "ignite": {
    "version": "9.7.2"
  }
}